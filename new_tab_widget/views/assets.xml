<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend_id" inherit_id="web.assets_backend" name="assets backend">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/new_tab_widget/static/src/js/tree_view.js"/>
        </xpath>
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/new_tab_widget/static/src/js/widget.js"
            />
        </xpath>
    </template>

</odoo>